  "dependencies": {
    "commander": "^13.1.0",
    "glob": "^11.0.1",
    "minimatch": "^10.0.1"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.1",
    "@types/node": "^22.12.0",
    "electron": "^34.5.0",
    "prettier": "^3.3.3",
    "typedoc": "~0.25.13",
    "typescript": "^5.5.4",
    "vitest": "^3.2.4",
    "xvfb-maybe": "^0.2.1"
  },
  "packageManager": "yarn@4.10.3+sha512.c38cafb5c7bb273f3926d04e55e1d8c9dfa7d9c3ea1f36a4868fa028b9e5f72298f0b7f401ad5eb921749eb012eb1c3bb74bf7503df3ee43fd600d14a018266f",
